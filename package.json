{
  "name": "@flocalhost/timekit",
  "description": "A lib for flocalhost plugin development",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
